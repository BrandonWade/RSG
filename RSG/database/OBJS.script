CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE SCENERY(GAMECODE VARCHAR(4) NOT NULL PRIMARY KEY,NAME VARCHAR(20),FILEPATH VARCHAR(40),TRAVERSABLE BOOLEAN,INTERACTMESSAGE VARCHAR(256),MAPICONPATH VARCHAR(60),SAILABLE BOOLEAN)
CREATE MEMORY TABLE ITEMS(GAMECODE VARCHAR(4) NOT NULL PRIMARY KEY,NAME VARCHAR(13),FILEPATH VARCHAR(40),INTERACTIVE BOOLEAN,VALUE INTEGER,INTERACTMESSAGE VARCHAR(256))
CREATE MEMORY TABLE MAPS(MAPPOS INTEGER NOT NULL PRIMARY KEY,FILEPATH VARCHAR(30),POSX INTEGER,POSY INTEGER)
CREATE MEMORY TABLE CHARACTERS(GAMECODE VARCHAR(4) NOT NULL PRIMARY KEY,NAME VARCHAR(20),FILEPATH VARCHAR(40),HEALTH INTEGER,MAXHEALTH INTEGER,VALUE INTEGER,INTERACTMESSAGE VARCHAR(256))
CREATE MEMORY TABLE POTIONS(GAMECODE VARCHAR(4) NOT NULL PRIMARY KEY,NAME VARCHAR(13),FILEPATH VARCHAR(40),INTERACTIVE BOOLEAN,VALUE INTEGER,INTERACTMESSAGE VARCHAR(256))
CREATE MEMORY TABLE WEAPONS(GAMECODE VARCHAR(4) NOT NULL PRIMARY KEY,NAME VARCHAR(13),FILEPATH VARCHAR(40),INTERACTIVE BOOLEAN,VALUE INTEGER,INTERACTMESSAGE VARCHAR(256))
CREATE MEMORY TABLE GOALS(GOALNUM INTEGER NOT NULL PRIMARY KEY,DESCRIPTION VARCHAR(100),GAMECODE VARCHAR(4),COMPLETED VARCHAR(100),WRONG VARCHAR(100))
CREATE MEMORY TABLE VEHICLES(GAMECODE VARCHAR(4) NOT NULL PRIMARY KEY,NAME VARCHAR(13),FILEPATH VARCHAR(40),INTERACTIVE BOOLEAN,VALUE INTEGER,INTERACTMESSAGE VARCHAR(256))
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 20
SET SCHEMA PUBLIC
